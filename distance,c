//calculates the distance to kisumu
#include <stdio.h>

int main()
{
    int speed = 0;
    int time = 0;


    printf("speed: ");
    scanf("%d",&speed);
    printf("time: ");
    scanf("%d",&time);

    int distance = speed * time;

    printf("the distance to kisumu is : %d",distance);

    return 0;
}
